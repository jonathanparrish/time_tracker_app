<h1>New Time Entry</h1>

<%= form_for @time_entry do |t|%>
  <div class="field">
    <%= t.label :developer_id %><br>
    <%= t.collection_select :developer_id, Developer.all, :id, :name, include_blank: "" %>
  </div>
  <div class="field">
    <%= t.label :hours %><br>
    <%= t.number_field :hours %>
  </div>
  <div class="field">
    <%= t.label :worked_on %><br>
    <%= t.date_field :worked_on %>
  </div>
  <div class="field">
    <%= t.label :project_id %><br>
    <%= t.collection_select :project_id, Project.all, :id, :name, include_blank: "" %>
  </div>
  <div class="actions">
    <%= t.submit %>
  </div>
<% end %>
