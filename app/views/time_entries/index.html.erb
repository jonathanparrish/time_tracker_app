<p id="notice"><%= notice %></p>

<h1>Time Entry list</h1>

<table>
  <thead>
    <tr>
      <th>Project ID</th>
      <th>Date Worked On</th>
      <th>Hours Worked</th>
      <th>Developer ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time| %>
      <tr>
        <td><%= time.project_id %></td>
        <td><%= time.worked_on %></td>
        <td><%= time.hours %></td>
        <td><%= time.developer_id %></td>
        <td><%= link_to 'Show', time %></td>
        <td><%= link_to 'Edit', edit_project_path(time) %></td>
        <td><%= link_to 'Destroy', time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Project Home', projects_path %>
<%= link_to 'New Time Entry', new_time_entry_path %>
<%= link_to 'Developer Home', developers_path %>
