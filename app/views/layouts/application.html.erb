<!DOCTYPE html>
<html>
  <head>
    <title>Time Tracker</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <p><%= notice %></p>
    <% if session[:developer_id] %>
      <%= button_to "Log out", login_path(session[:developer_id]), method: :delete %>
    <% end %>

    <%= yield %>
  </body>
</html>
