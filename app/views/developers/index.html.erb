<h1>List of Developers</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Total Hours Worked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developers.each do |developer| %>
      <tr>
        <td><%= developer.id %></td>
        <td><%= developer.name %></td>
        <td><%= developer.email %></td>
        <td><%= developer.total_hours %></td>
        <td><%= link_to 'Show', developer %></td>
        <td><%= link_to 'Edit', edit_developer_path(developer) %></td>
        <td><%= link_to 'Destroy', developer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Project Home', projects_path %>
<%= link_to 'Time Entry Home', time_entries_path %>
<%= link_to 'New Developer', new_developer_path %>
